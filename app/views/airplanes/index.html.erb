<div class="container my-3">
  <h1>Airplanes</h1>
  <% @airplanes.each do |airplane| %>
    <div class="card my-3">
      <div class="card-body">
        <h5 class="card-title"><%= airplane.name %></h5>
        <%= cl_image_tag(airplane.photo.key, height: 300, width: 400, crop: :fill)%>
      </div>
    </div>
  <% end %>
  <%= render "form"%>
</div>
